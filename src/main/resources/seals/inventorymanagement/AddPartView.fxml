      </VBox>
      <TextField fx:id="partIdTextField" disable="true" layoutX="156.0" layoutY="151.0" promptText="Auto Gen - Disabled" />
      <TextField fx:id="partNameTextField" layoutX="156.0" layoutY="182.0" />
      <TextField fx:id="partInvTextField" layoutX="156.0" layoutY="214.0" />
      <TextField fx:id="partPriceTextField" layoutX="156.0" layoutY="245.0" />
      <TextField fx:id="partMaxTextField" layoutX="156.0" layoutY="276.0" />
      <TextField fx:id="machineIDCompanyNameTextField" layoutX="156.0" layoutY="307.0" />
      <TextField fx:id="partMinTextField" layoutX="362.0" layoutY="276.0" />
      <Button layoutX="285.0" layoutY="408.0" mnemonicParsing="false" onAction="#addPartSaveBtnPushed" text="Save" />
      <Button layoutX="347.0" layoutY="408.0" mnemonicParsing="false" onAction="#fromAddPartToMainBtnPushed" text="Cancel" />
      <Label fx:id="machineIDCompanyNameLabel" layoutX="51.0" layoutY="311.0" prefHeight="17.0" prefWidth="97.0" text="Machine ID" textOverrun="CLIP" />
   </children>
