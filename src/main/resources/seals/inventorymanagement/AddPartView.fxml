      <Label fx:id="partMinLabel" layoutX="326.0" layoutY="280.0" text="Min" />
      <VBox layoutX="49.0" layoutY="151.0" pickOnBounds="false" prefHeight="257.0" prefWidth="103.0" spacing="15.0">
         <children>
            <Label text="ID" />
            <Label text="Name" />
            <Label text="Inv" />
            <Label text="Price/Cost" />
            <Label text="Max" />
